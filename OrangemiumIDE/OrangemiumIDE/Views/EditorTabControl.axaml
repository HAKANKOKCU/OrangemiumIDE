<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OrangemiumIDE.Views.EditorTabControl">
  <DockPanel>
    <Panel Name="sizePanel" Background="Transparent" Width="3" DockPanel.Dock="Left" Cursor="SizeWestEast"></Panel>
    <TabControl Classes="draggable" Name="tabControl" Padding="0" Margin="0" AutoScrollToSelectedItem="True"></TabControl>
  </DockPanel>
</UserControl>
