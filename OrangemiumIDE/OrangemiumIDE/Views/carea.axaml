<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OrangemiumIDE.Views.carea">
  <DockPanel>
    <DockPanel LastChildFill="False" DockPanel.Dock="Top">
      <StackPanel DockPanel.Dock="Right" Orientation="Horizontal">
        <Button Padding="0" Width="20" Height="20" Background="Transparent" Name="cbtn" HorizontalContentAlignment="Center">x</Button>
      </StackPanel>
    </DockPanel>  
    <TabControl Name="consoletc" Height="250" Padding="0">
				<TabItem Header="Problems">
          <ScrollViewer>
            <StackPanel Orientation="Vertical" Name="probstack"></StackPanel>  
          </ScrollViewer>
        </TabItem>
		</TabControl>
  </DockPanel>
</UserControl>
