<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OrangemiumIDE.Views.problemli">
  <Button Name="btn" Margin="2" Padding="4" HorizontalAlignment="Stretch" Background="Transparent">
    <DockPanel>
      <Image Width="16" Height="16" VerticalAlignment="Center" Name="ico"></Image>
      <Label Name="loclabel" VerticalAlignment="Center" Foreground="Orange" Padding="5"></Label>
      <TextBlock Name="msg" VerticalAlignment="Center" Padding="5"></TextBlock>
    </DockPanel>
  </Button>
</UserControl>
